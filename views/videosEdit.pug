extends layouts/main
block content
  .video-upload
    h3 비디오 수정
    form(action=routes.home, method="post")
      input(type="text", name="videoTitle", placeholder="제목")
      input(type="textarea", name="videoDesc", placeholder="설명")
      a(href=routes.deleteVideo) 비디오 삭제
      input(type="submit", value="수정 완료")